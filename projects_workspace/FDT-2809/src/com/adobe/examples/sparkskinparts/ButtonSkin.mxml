<?xml version="1.0" encoding="utf-8"?>
<s:SparkSkin xmlns:fx="http://ns.adobe.com/mxml/2009" xmlns:s="library://ns.adobe.com/flex/spark"
	xmlns:fb="http://ns.adobe.com/flashbuilder/2009" minWidth="100"
	minHeight="100" alpha.disabled="0.5">

	<fx:Metadata>
		[HostComponent("spark.components.Button")]
	</fx:Metadata>

	<fx:Script fb:purpose="styling">
	</fx:Script>

	<s:states>
		<s:State name="up"/>
		<s:State name="over"/>
		<s:State name="down"/>
		<s:State name="disabled"/>
	</s:states>

	<s:Rect height="100%" width="100%" includeIn="over">
		<s:fill>
			<s:SolidColor color="blue" alpha=".5"/>
		</s:fill>
	</s:Rect>

	<s:Label id="labelElement" textAlign="center" verticalAlign="middle"
		lineBreak="toFit" horizontalCenter="0" verticalCenter="0"
		text="{hostComponent.label}">
	</s:Label>

</s:SparkSkin>
